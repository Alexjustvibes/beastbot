{
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "import/no-unresolved": 0,
    "quotes": [
      "warn",
      "double",
      {
        "avoidEscape": true
      }
    ],
    "spaced-comment": "off",
    "object-curly-spacing": ["warn", "never"],
    "indent": ["warn", 4]
  }
}
